; Counts to 300
set BH 0x0
set BL 0x0

; Increment register
set CL 0x1

; Compare register
set DH 0x1
set DL 0x2C
; set DH 0x0
; set DL 0xA

; Set mode to unsigned 16 bit int
set MODE 0x1
; Set compare flag to GTE
set CMPF 0x8

begin:
; Set "output" register G
move BH GH
move BL GL

; Increment B
add BH CH BH
cmp BH DH EH
jump :end EH
jump :begin 1

end:
stop